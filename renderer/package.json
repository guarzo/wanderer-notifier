  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch --minify false --emptyOutDir false --mode development",
    "dev:watch": "npm run watch & npx onchange 'dist/**/*' -- cp -r dist/* ../priv/static/app/",
    "dev:sync": "node watch-and-copy.js",
    "sync": "node watch-sync.js",
    "lint": "eslint .",
    "postbuild": "rm -rf ../priv/static/app && mkdir -p ../priv/static/app && cp -r dist/* ../priv/static/app/",
    "preview": "vite preview",
    "chart-service": "node chart-service/chart-generator.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
