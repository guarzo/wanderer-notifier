    "@mui/material": "^7.1.0",
    "@mui/x-date-pickers": "^8.3.1",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/react-query": "^5.76.1",
    "body-parser": "^2.2.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.0",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tailwindcss/postcss": "^4.1.7",
